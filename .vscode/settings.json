{
    "terminal.integrated.copyOnSelection": true,
    "vscode-postgres.defaultConnection": "humlabseadserv.srv.its.umu.se",
	"vscode-postgres.defaultDatabase": "sead_staging",
    "vscode-postgres.prettyPrintJSONfields": true,
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#b68f64",
        "activityBar.background": "#b68f64",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#bee0d0",
        "activityBarBadge.foreground": "#15202b",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#b68f64",
        "statusBar.background": "#9d764a",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#b68f64",
        "statusBarItem.remoteBackground": "#9d764a",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#9d764a",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#9d764a99",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.remoteColor": "#9d764a",
    "Prettier-SQL.keywordCase": "lower",
    "Prettier-SQL.SQLFlavourOverride": "postgresql",
    "Prettier-SQL.commaPosition": "after",
    "Prettier-SQL.indentStyle": "standard",
    "Prettier-SQL.logicalOperatorNewline": "before",
    "Prettier-SQL.newlineBeforeSemicolon": false,
    "Prettier-SQL.tabulateAlias": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.hg/store/**": true,
        "**/logs/**": true,
    }
}